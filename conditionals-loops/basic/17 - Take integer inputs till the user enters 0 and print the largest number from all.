import java.util.Scanner;

public class gihub{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int large = 0;
		while(true) {
			System.out.print("Enter A Number (0 to exit) - ");
			int num = sc.nextInt();

			if(num>large) {
				large=num;
			}else if(num==0) {
				System.out.println("End");
				break;
			}
		}	
		if(large==0) {
			System.out.println("No Largest Number is enter");
		}else {
			System.out.println(large+" is a Largest Number");
		}

	}
}
